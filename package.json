{
  "name": "react-begin",
  "version": "1.0.0",
  "description": "a react project demo",
  "private": true,
  "scripts": {
    "update": "ncu -u && npm install",
    "eslint": "esw -w src/*",
    "clean:dist": "rm -rf dist/js & rm -rf dist/styles & rm -rf dist/index.html & rm -rf dist/fonts",
    "build": "npm run clean:dist && NODE_ENV=prod webpack --config webpack.config.babel.js --progress --colors",
    "start": "NODE_ENV=dev webpack-dev-server --config webpack.config.babel.js --watch-poll --inline --hot"
  },
  "keywords": [
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:liu-dongyu/react-begin.git"
  },
  "author": "https://github.com/liu-dongyu",
  "license": "ISC",
  "devDependencies": {
