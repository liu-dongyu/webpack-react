    "webpack": "^1.12.14",
    "webpack-config": "^5.0.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "font-awesome": "^4.5.0",
    "immutable": "^3.7.6",
    "normalize.css": "^4.1.1",
    "normalizr": "^2.0.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
